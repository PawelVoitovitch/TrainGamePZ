</head>

<body>

    <!-- @include 'nav.kit' -->
    <main class="register">
        <div class="wrapper">

            <div class="mainRegister">
                <!-- @include 'loginForm.kit' -->

                <section class="registerForm">
                    <input type="text" placeholder="Login" id="login" class="inputRegister">
                    <input type="email" placeholder="Email" id="email" class="inputRegister">
                    <input type="password" placeholder="Password" id="password" class="inputRegister">
                    <input type="password" placeholder="Confirm Password" id="confirmPassword" class="inputRegister">
                    <div class="checkTerms inputRegister">
                        <input type="checkbox" id="terms">
                        <label for="terms"> I accept terms and conditions</label>
                    </div>
                    <button class="registerButton inputRegister">Register</button>
                </section>
            </div>
        </div>
    </main>


    <script src="./dist/js/login.js"></script>
</body>

</html>