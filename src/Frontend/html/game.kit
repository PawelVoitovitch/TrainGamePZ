<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>KolejÄ… przez stany | Game</title>
    <link rel="stylesheet" href="../dist/css/setRoutes.css">
    <!-- @include 'link.kit' -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/stomp.js/2.3.3/stomp.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/sockjs-client/1.5.1/sockjs.min.js"></script>
</head>

<body>
    <main class="gameMain">

        <div class="tableName">
            <p class="tableName-name">Table name</p>
        </div>
        <div class="wrapper">
            <div class="table">
                <div class="alertPopup">
                    <p class="alertPopup_text">Text</p>
                    <button class="alertPopup_btn">OK</button>
                </div>
                <section class="gameResources">
                    <p class="title">Game Resources</p>
                    <div class="trainCardsActive">
                        <img src="" class="card one" alt="train card">
                        <img src="" class="card two" alt="train card">
                        <img src="" class="card three" alt="train card">
                        <img src="" class="card four" alt="train card">
                        <img src="" class="card five" alt="train card">
                    </div>
                    <div class="matchingCards">
                        <div class="cards">
                            <p class="title">TrainCards</p>
                            <div class="box">
                                <img src="../dist/img/Train1.png" class="matchingCard matchTickets" alt="train cards">
                                <p class="quantity trainCardsQuantityAll">x87</p>
                            </div>
                        </div>
                        <div class="cards">
                            <p class="title">Destination Tickets</p>
                            <div class="box">
                                <img src="../dist/img/Ticket1.png" class="matchingCard matchDestination"
                                    alt="destination cards">
                                <p class="quantity destinationCardsQuantityAll">x24</p>
                            </div>
                        </div>
                    </div>
                </section>

                <section class="boardArea">
                    <div class="mapBox">
                        <img src="../dist/img/GameMap.png" alt="plansza" class="board">
                        <!-- @include 'routes.kit' -->
                    </div>
                    <div class="resources">
                        <div class="leftPart">
                            <div class="destinationTickets">

                                <img src="../dist/img/Ticket.png" class="destinationTicket" alt="tickety lokalizacji">
                            </div>
                            <div class="cards">
                                <div class="cardBox">
                                    <img src="../dist/img/TrainBlueCard1.png" class="card blue" alt="train card">
                                    <p class="cardsQuantity">0</p>
                                </div>
                                <div class="cardBox">
                                    <img src="../dist/img/TrainBlackCard1.png" class="card black" alt="train card">
                                    <p class="cardsQuantity">0</p>
                                </div>
                                <div class="cardBox">
                                    <img src="../dist/img/TrainGreenCard1.png" class="card green" alt="train card">
                                    <p class="cardsQuantity">0</p>
                                </div>
                                <div class="cardBox">
                                    <img src="../dist/img/TrainOrangeCard1.png" class="card orange" alt="train card">
                                    <p class="cardsQuantity">0</p>
                                </div>
                                <div class="cardBox">
                                    <img src="../dist/img/TrainPinkCard1.png" class="card pink" alt="train card">
                                    <p class="cardsQuantity">0</p>
                                </div>
                                <div class="cardBox">
                                    <img src="../dist/img/TrainRainbowCard1.png" class="card rainbow" alt="train card">
                                    <p class="cardsQuantity">0</p>
                                </div>
                                <div class="cardBox">
                                    <img src="../dist/img/TrainRedCard1.png" class="card red" alt="train card">
                                    <p class="cardsQuantity">0</p>
                                </div>
                                <div class="cardBox">
                                    <img src="../dist/img/TrainWhiteCard1.png" class="card white" alt="train card">
                                    <p class="cardsQuantity">0</p>
                                </div>
                                <div class="cardBox">
                                    <img src="../dist/img/TrainYellowCard1.png" class="card yellow" alt="train card">
                                    <p class="cardsQuantity">0</p>
                                </div>
                            </div>
                        </div>
                        <div class="rightPart">
                            <div class="pointsBox myBox">
                                <span class="mdi mdi-train-car-box icon"></span>
                                <p class="points">30</p>
                                <p class="playerName yourName">Your Name</p>
                                <p class="trains">
                                    <span class="trainsQuantity">45</span>x Trains
                                </p>
                            </div>

                        </div>
                    </div>
                </section>
                <section class="rivalsPoint">
                    <div class="box playersBox">

                        <p class="playerName oponentName"></p>
                        <p class="oponentTrains">
                            <span class="oponentsTrainsQuantity"></span>x Trains
                        </p>
                        <p class="destination">
                            <span class="destinationQuantity"></span>x Destination Tickets
                        </p>
                        <p class="trainCards">
                            <span class="trainCardQuantity"></span>x Trains Tickets
                        </p>
                        <p class="oponentPoints"></p>
                    </div>
                    <div class="box playersBox">

                        <p class="playerName oponentName"></p>
                        <p class="oponentTrains">
                            <span class="oponentsTrainsQuantity"></span>x Trains
                        </p>
                        <p class="destination">
                            <span class="destinationQuantity"></span>x Destination Tickets
                        </p>
                        <p class="trainCards">
                            <span class="trainCardQuantity"></span>x Trains Tickets
                        </p>
                        <p class="oponentPoints"></p>
                    </div>
                    <div class="box playersBox">

                        <p class="playerName oponentName"></p>
                        <p class="oponentTrains">
                            <span class="oponentsTrainsQuantity"></span>x Trains
                        </p>
                        <p class="destination">
                            <span class="destinationQuantity"></span>x Destination Tickets
                        </p>
                        <p class="trainCards">
                            <span class="trainCardQuantity"></span>x Trains Tickets
                        </p>
                        <p class="oponentPoints"></p>
                    </div>
                </section>

            </div>
            <div class="destinationPopup">
                <p class="destinationPopupText">You need to select at least&nbsp; <span
                        class="counter">2</span>&nbsp;more cards</p>
                <button class="sendDestinationCards">CHOOSE</button>
                <p class="whoseTurn"></p>
                <div class="popupCardsBox">
                    <img src="../dist/img/ExampleCard.png" alt="destination card" class="popupCard one">
                    <img src="../dist/img/ExampleCard.png" alt="destination card" class="popupCard two">
                    <img src="../dist/img/ExampleCard.png" alt="destination card" class="popupCard three">
                </div>
            </div>
            <div class="destinationPopupAfterClick">
                <p class="destinationPopupText">You need to select at least&nbsp; <span
                        class="counterAfterClick">1</span>&nbsp;more cards</p>
                <button class="sendDestinationCardsAfterClick">CHOOSE</button>
                <div class="popupCardsBoxAfterClick">
                    <img src="../dist/img/ExampleCard.png" alt="destination card" class="popupCardAfterClick one">
                    <img src="../dist/img/ExampleCard.png" alt="destination card" class="popupCardAfterClick two">
                    <img src="../dist/img/ExampleCard.png" alt="destination card" class="popupCardAfterClick three">
                </div>
            </div>
            <div class="yourDestinationTickets">
                <h2 class="yourDestinationTicketsText">Your tickets</h2>
                <button class="closeBtnTickets mdi mdi-close"></button>
                <div class="popupYourTicketCardsBox">
                    <img src="../dist/img/ExampleCard.png" alt="destination card" class="popupYourTicketCard one">
                    <img src="../dist/img/ExampleCard.png" alt="destination card" class="popupYourTicketCard two">
                    <img src="../dist/img/ExampleCard.png" alt="destination card" class="popupYourTicketCard three">

                </div>
            </div>
            <div class="trainPopup">
                <h2 class="trainPopupText">Choose one color to set route</h2>
                <div class="trainPopupCardsBox">

                </div>
            </div>
            <div class="turnPopup">
            </div>
            <div class="resultPopup">
                <span class="mdi mdi-close" id="closeResultPopup"></span>
                <h2 class="trainPopupText">Results</h2>

            </div>
        </div>

    </main>
</body>
<script src="../dist/js/websocket.js"></script>
<script src="../dist/js/popupAlert.js"></script>
<script src="../dist/js/game.js"></script>
<script src="../dist/js/ticketCards.js"></script>
<script src="../dist/js/setUsers.js"></script>
<script src="../dist/js/trainCards.js"></script>
<script src="../dist/js/setRoutes.js"></script>

<script src="../dist/js/turn.js"></script>

</html>