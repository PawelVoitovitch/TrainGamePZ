<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>KolejÄ… przez stany | Lobby</title>
    <!-- @include 'link.kit' -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/stomp.js/2.3.3/stomp.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/sockjs-client/1.5.1/sockjs.min.js"></script>
</head>

<body>
    <!-- @include 'nav.kit' -->

    <main class="mainCreate">
        <div class="wrapper">
            <div class="main">
                <!-- @include 'loginForm.kit' -->
                <div class="mainCreateBox">

                    <div class="alertPopup">
                        <p class="alertPopup_text">Text</p>
                        <button class="alertPopup_btn">OK</button>
                    </div>

                    <section class="boxLeft">
                        <h1 class="boxLeft_title" id="gameTitle">
                            Your Name's Table
                        </h1>
                        <div class="boxLeft_users">
                        </div>
                    </section>

                    <section class="boxRight">
                        <div class="boxRight_game-setup">
                            <img src="../dist/img/logo-white.png" alt="game logo" class="logoLobby">
                            <div class="boxRight_game-setup-inviteLink">
                                <label for="inviteLink">Invite:</label>
                                <p class="inviteLinkText" id="inviteLink">https://ride.gg/ab3Fsg</p>
                            </div>
                        </div>
                        <div class="boxRight_game-buttons">
                            <button class="boxRight_game-buttons-button start_game"> <a href="#" class="link "
                                    id="startButton">Start
                                    Game</a></button>
                        </div>
                    </section>
                </div>
            </div>
        </div>

    </main>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/stomp.js/2.3.3/stomp.min.js"></script>
    <script src="../dist/js/websocket.js"></script>
    <script src="../dist/js/lobby.js"></script>
    <script src="../dist/js/login.js"></script>
    <script src="../dist/js/main.js"></script>
</body>

</html>